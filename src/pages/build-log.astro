<style>
  .journal-wrap {
    max-width: 820px;
    margin: 0 auto;
    padding: 0 16px;
    overflow-x: hidden;
  }
  .journal-header {
    font-family: var(--font-body);
    color: var(--text-dim);
    padding: 12px 0;
    font-size: 0.9rem;
    border: none;
    background: none;
    white-space: pre-wrap;
  }
  .journal-day {
    padding: 24px 0;
    border-top: 1px solid #1a1a1a;
  }
  .journal-day.genesis {
    border-top: 1px solid var(--ice-dim);
  }
  .journal-date {
    font-family: var(--font-head);
    font-weight: 700;
    font-size: 1.2rem;
    letter-spacing: 0.05em;
    margin-bottom: 16px;
  }
  pre.j {
    font-family: var(--font-body);
    font-size: 0.95rem;
    line-height: 1.75;
    color: var(--text-dim);
    white-space: pre-wrap;
    word-break: break-word;
    margin: 0;
    padding: 0;
    border: none;
    background: none;
  }
  .journal-metrics {
    margin-top: 16px;
    padding: 12px 16px;
    border: 1px solid #1a1a1a;
    background: #080808;
    color: var(--text-dim);
    font-family: var(--font-body);
    font-size: 0.85rem;
    letter-spacing: 0.02em;
  }
  .tag-shipped { color: var(--phosphor); font-weight: 700; }
  .tag-built { color: var(--phosphor); }
  .tag-live { color: var(--phosphor); }
  .tag-fixed { color: var(--amber); font-weight: 700; }
  .tag-purge { color: var(--amber); }
  .tag-killed { color: var(--red); font-weight: 700; }
  .tag-failed { color: var(--red); }
  .blink-cursor { animation: blk 1s step-end infinite; }
  @keyframes blk { 0%, 100% { opacity: 1; } 50% { opacity: 0; } }
  @media (max-width: 640px) {
    .journal-wrap { padding: 0 10px; }
    .journal-day { padding: 18px 0; }
    pre.j { font-size: 0.82rem; line-height: 1.65; }
    .journal-date { font-size: 1rem; }
    .journal-metrics { font-size: 0.78rem; padding: 10px 12px; }
  }
</style>
