<style>
  .journal-wrap {
    max-width: 820px;
    padding: 0 var(--sp-16);
    overflow-x: hidden;
  }
  .journal-header {
    font-family: var(--font-body);
    color: var(--text-dim);
    padding: var(--sp-12) 0;
    font-size: var(--size-md);
    border: none;
    background: none;
    white-space: pre-wrap;
  }
  .journal-day {
    padding: var(--sp-24) 0;
    border-top: var(--border-panel);
  }
  .journal-day.genesis {
    border-top: 1px solid var(--ice-dim);
  }
  .journal-date {
    font-family: var(--font-head);
    font-weight: 700;
    font-size: var(--size-xl);
    letter-spacing: 0.05em;
    margin-bottom: var(--sp-16);
  }
  pre.j {
    font-family: var(--font-body);
    font-size: var(--size-md);
    line-height: 1.75;
    color: var(--text-dim);
    white-space: pre-wrap;
    word-break: break-word;
    margin: 0;
    padding: 0;
    border: none;
    background: none;
  }
  .journal-metrics {
    margin-top: var(--sp-16);
    padding: var(--sp-12) var(--sp-16);
    border: var(--border-panel);
    background: var(--surface);
    color: var(--text-dim);
    font-family: var(--font-body);
    font-size: var(--size-base);
    letter-spacing: 0.02em;
  }
  .tag-shipped { color: var(--phosphor); font-weight: 700; }
  .tag-built { color: var(--phosphor); }
  .tag-live { color: var(--phosphor); }
  .tag-fixed { color: var(--amber); font-weight: 700; }
  .tag-purge { color: var(--amber); }
  .tag-killed { color: var(--red); font-weight: 700; }
  .tag-failed { color: var(--red); }
  .blink-cursor { animation: blk 1s step-end infinite; }
  @keyframes blk { 0%, 100% { opacity: 1; } 50% { opacity: 0; } }
  @media (max-width: 640px) {
    .journal-wrap { padding: 0 var(--sp-10); }
    .journal-day { padding: 18px 0; }
    pre.j { font-size: var(--size-base); line-height: 1.65; }
    .journal-date { font-size: var(--size-lg); }
    .journal-metrics { font-size: var(--size-sm); padding: var(--sp-10) var(--sp-12); }
  }
</style>
