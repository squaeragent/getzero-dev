<style>
  .build-log-wrap {
    max-width: 100%;
    overflow-x: hidden;
  }
  .prompt-line {
    padding: 8px 12px;
  }
  .day-entry {
    border-top: 1px solid #1a1a1a;
    padding: 8px 12px;
  }
  .day-entry.genesis {
    border-top: 1px solid var(--ice-dim);
  }
  .day-header {
    font-family: var(--font-head);
    font-weight: 700;
    font-size: 0.9rem;
    margin-bottom: 6px;
    padding-bottom: 4px;
    border-bottom: 1px solid #1a1a1a;
  }
  .day-body {
    font-family: var(--font-body);
    font-size: 0.85rem;
    color: var(--text-dim);
    word-break: break-word;
    line-height: 1.45;
  }
  .day-summary {
    color: var(--text);
    margin-bottom: 4px;
  }
  .day-metrics {
    margin-top: 6px;
    padding-top: 4px;
    border-top: 1px solid #111;
    color: var(--text-dim);
    white-space: pre-wrap;
  }
  .li {
    padding-left: 1.4em;
    text-indent: -1.4em;
    margin-bottom: 2px;
    line-height: 1.45;
    word-break: break-word;
  }
  .blink-cursor { animation: blk 1s step-end infinite; }
  @keyframes blk { 0%, 100% { opacity: 1; } 50% { opacity: 0; } }
  @media (max-width: 640px) {
    .day-entry { padding: 6px 8px; }
    .day-body { font-size: 0.8rem; }
    .day-header { font-size: 0.8rem; }
  }
</style>

<style>
  h2.day-header { margin: 0; }
</style>
